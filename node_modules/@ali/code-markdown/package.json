{
  "name": "@ali/code-markdown",
  "version": "2.0.39",
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "description": "a light-weight Markdown editor based on React",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "unpkg": "lib/index.js",
  "jsdelivr": "lib/index.js",
  "files": [
    "cjs",
    "esm",
    "lib",
    "preview",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "build-scripts start",
    "build": "set GENERATE_SOURCEMAP=false && build-scripts build",
    "prod": "build-scripts build",
    "test": "mocha",
    "coverage": "nyc mocha",
    "postversion": "git push --follow-tags --no-verify"
  },
  "keywords": [
    "markdown",
    "html",
    "editor",
    "parser",
    "react",
    "component",
    "plugins",
    "pluggable"
  ],
  "author": "HarryChen && ShuangYa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarryChen0506/react-markdown-editor-lite/issues"
  },
  "homepage": "https://unpkg.alibaba-inc.com/@ali/code-markdown@2.0.38/build/index.html",
  "devDependencies": {
    "@alib/build-scripts": "^0.1.3",
    "@iceworks/spec": "^1.0.0",
    "@testing-library/react": "^10.2.1",
    "@types/chai": "^4.2.7",
    "@types/classnames": "^2.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.5.1",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/markdown-it": "^0.0.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "build-plugin-component": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^10.0.0",
    "husky": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.2",
    "highlight.js": "^11.6.0",
    "khroma": "^2.0.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.3.1",
    "source-map-support": "^0.5.16",
    "stylelint": "^13.7.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@liradb2000/markdown-it-mermaid": "^0.4.4",
    "classnames": "^2.2.6",
    "eventemitter3": "^4.0.0",
    "markdown-it": "8.4.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-diagrams": "^1.0.1",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-graphviz": "^1.1.2",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-mathjax3": "^4.3.1",
    "markdown-it-multimd-table": "^4.1.3",
    "markdown-it-plantuml": "^1.4.1",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table": "^2.0.4",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.2.0",
    "textarea-caret": "^3.1.0",
    "textarea-caret-position": "^0.1.1",
    "uslug": "^1.0.4",
    "uuid": "^8.3.2"
  }
}

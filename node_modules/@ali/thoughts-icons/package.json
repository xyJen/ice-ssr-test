{
  "name": "@ali/thoughts-icons",
  "version": "1.9.3",
  "description": "Icons for Thoughts",
  "keywords": [
    "iconfont"
  ],
  "author": "Washington Hua",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.alibaba-inc.com/thoughts/thoughts-icons.git"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "setup": "ts-node --project tsconfig.setup.json scripts/setup.ts",
    "demo": "npm-run-all build:demo serve",
    "dev": "npm-run-all build:bundle clear:demo dev:demo",
    "dev:demo": "rollup -c -w --configDemo --configDev",
    "build": "npm-run-all build:bundle build:demo",
    "compile:bundle": "rollup -c --configBundle",
    "compile:demo": "rollup -c --configDemo",
    "build:bundle": "npm-run-all setup clear:bundle compile:bundle",
    "build:demo": "npm-run-all setup clear:demo compile:demo",
    "clear": "npm-run-all clear:bundle clear:demo",
    "clear:bundle": "rimraf lib es",
    "clear:demo": "rimraf demo/dist",
    "serve": "serve demo/dist",
    "release": "standard-version",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "release:alpha": "npm run release -- --prerelease alpha",
    "release:beta": "npm run release -- --prerelease beta"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/react": "^16.9.21",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.19.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-extensions": "^0.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "sass": "^1.25.0",
    "serve": "^11.3.0",
    "standard-version": "^8.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
